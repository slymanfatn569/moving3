# متغيرات بيئية للإنتاج على Netlify

# عنوان الموقع (قم بتعديله عند شراء النطاق)
NEXT_PUBLIC_SITE_URL=https://fakhrkhaleej.netlify.app
NEXT_PUBLIC_DOMAIN=fakhrkhaleej.netlify.app

# بيئة التشغيل
NODE_ENV=production

# إعدادات الموقع
NEXT_PUBLIC_SITE_NAME=فخر الخليج
NEXT_PUBLIC_DEFAULT_LOCALE=ar
NEXT_PUBLIC_SITE_DESCRIPTION=مزود رائد لخدمات نقل العفش والأثاث في جميع أنحاء المملكة العربية السعودية، نقدم خدمات احترافية للأفراد والشركات والمؤسسات.
NEXT_PUBLIC_SITE_KEYWORDS=نقل عفش، نقل أثاث، المملكة العربية السعودية، خدمات التخزين، فك وتركيب الأثاث

# إعدادات تحسين الأداء
NEXT_PUBLIC_ENABLE_IMAGE_OPTIMIZATION=true
NEXT_PUBLIC_ENABLE_PREFETCHING=true

# إعدادات تحليلات جوجل (إذا كانت مطلوبة)
# NEXT_PUBLIC_GA_TRACKING_ID=UA-XXXXXXXXX-X

# إعدادات خرائط جوجل (إذا كانت مطلوبة)
# NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_API_KEY

# تكوين Netlify
NETLIFY=true
NETLIFY_NEXT_PLUGIN_SKIP=true

# خدمات AWS (إذا كانت مطلوبة)
# AWS_S3_BUCKET=
# AWS_REGION=

# إعدادات أخرى
NEXT_PUBLIC_CONTACT_EMAIL=info@fakhrkhaleej.com
NEXT_PUBLIC_CONTACT_PHONE=+966XXXXXXXXX 